{
  "name": "student-assistant",
  "version": "2.3.0",
  "description": "Student Assistant - AI-powered learning tool with transcription, flashcards, notes, and document support (PDF/PowerPoint)",
  "type": "module",
  "scripts": {
    "dev": "npm run frontend",
    "frontend": "python3 -m http.server 8000",
    "server": "cd server && npm start",
    "server:dev": "cd server && npm run dev",
    "start": "bash scripts/start.sh",
    "start:offline": "bash scripts/start-offline.sh",
    "stop": "bash scripts/stop.sh",
    "install:server": "cd server && npm install",
    "check-backend": "bash scripts/check-backend.sh",
    "download-models": "bash scripts/download-whisper-tiny-onnx.sh",
    "test:offline": "open http://localhost:8000/test-offline.html",
    "clean": "rm -rf server/node_modules node_modules",
    "docs": "open docs/README.md",
    "docs:offline": "open docs/OFFLINE_GUIDE.md"
  },
  "keywords": [
    "education",
    "ai",
    "transcription",
    "flashcards",
    "notes",
    "whisper",
    "student",
    "srs",
    "spaced-repetition",
    "learning",
    "indexeddb",
    "pwa"
  ],
  "author": "himusuwu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/himusuwu/student_asystent.git"
  },
  "bugs": {
    "url": "https://github.com/himusuwu/student_asystent/issues"
  },
  "homepage": "https://github.com/himusuwu/student_asystent#readme"
}
